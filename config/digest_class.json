{
    "system": {
        "net": ["classifier","net"],
        "time": null,
        "gpu": [0,1,2,3],
        "have_loss_weight":false,
        "net_param":{
            "backbone_position":["utils.backbone.Efficientnet","Efficientnet"],
            "param":{
                "pretrained":true,
                "name":"efficientnet-b0",
                "num_classes":1
            }
        },
        "loss_param":{
        },
        "evaluate_param":{
            "th":0.4
        }
    },
    "optimizer":{
        "type": "adam",
        "learning_rate": 1e-3,
        "adam":{"weight_decay":0},
        "sgd":{"weight_decay": 1e-5,"momentum":0.9},

        "decay_rate": 0.1,
        "milestones": [50,100,150,200],

        "test_on_train": false,
        "eval_on_train": false,
        "max_epoch": 250,
        "max_batch": null,
        "save_epoch": 10
    },
    "dataset": {
        "file_name": "digest_class",
        "num_workers":8,
        "shuffle_name": "shuffle_idx2.pt",
        "direction": ["%DATA%","digest"],
        "Pos_Neg_sample_rate": [1,1],
        "max_batch": 100,
        "train_rate": 0.8,
        "patch_size": 512,
        "stride_rate": 0.9,

        "pure_pos": true,
        "positive_picture_only":true,
        "train":{
            "batch_size": 40
        },
        "test":{
            "whole_image": false,
            "batch_size": 8
        }
    }
}